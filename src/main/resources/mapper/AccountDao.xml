<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jyp.chatroom.Dao.AccountDao">
    <select id="getById" resultType="Account" parameterType="int">
        <!-- 具体的sql -->
        SELECT * FROM account WHERE id = #{id}
    </select>

    <select id="getByName" resultType="Account" parameterType="String">
        SELECT * FROM  account WHERE name = #{name}
    </select>


</mapper>